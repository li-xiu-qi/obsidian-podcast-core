{
	"name": "obsidian-podcast-core",
	"version": "0.01",
	"description": "Transform your notes into engaging podcasts using AI - Generate scripts and audio with customizable voices",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",

	},
	"devDependencies": {
		"@types/node": "^22.8.7",
		"@typescript-eslint/eslint-plugin": "8.15.0",
		"@typescript-eslint/parser": "8.15.0",
		"@vicons/ionicons5": "^0.12.0",
		"@vicons/utils": "^0.1.4",
		"@vue/compiler-sfc": "^3.4.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.24.0",
		"esbuild-plugin-vue3": "^0.4.2",
		"naive-ui": "^2.38.0",
		"obsidian": "latest",
		"tslib": "2.8.1",
		"typescript": "5.6.3"
